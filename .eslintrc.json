{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":0, 
        "import/no-unresolved":0,
        "import/no-extraneous-dependencies": 0,
        "no-undef": 0,
        "array-callback-return": 0,
        "consistent-return": 0
    }
}